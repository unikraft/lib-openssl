################################################################################
# Library registration
################################################################################
$(eval $(call addlib_s,libssl,$(CONFIG_LIBSSL)))
$(eval $(call addlib_s,libcrypto,$(CONFIG_LIBCRYPTO)))

################################################################################
# Original sources
################################################################################
LIBOPENSSL_MAJOR=1.1.1
LIBOPENSSL_MINOR=m
LIBOPENSSL_VERSION=$(LIBOPENSSL_MAJOR)$(LIBOPENSSL_MINOR)
LIBOPENSSL_BASENAME=openssl-$(LIBOPENSSL_VERSION)
LIBOPENSSL_URL=https://www.openssl.org/source/openssl-$(LIBOPENSSL_VERSION).tar.gz
LIBOPENSSL_PATCHDIR=$(LIBSSL_BASE)/patches
$(eval $(call fetch,libssl,$(LIBOPENSSL_URL)))
$(eval $(call patch,libssl,$(LIBOPENSSL_PATCHDIR),$(LIBOPENSSL_BASENAME)))

################################################################################
# Helpers
################################################################################
LIBOPENSSL_SRC = $(LIBSSL_ORIGIN)/$(LIBOPENSSL_BASENAME)

################################################################################
# Library includes
################################################################################
LIBOPENSSL_GLOBAL_INCLUDES-y     += -I$(LIBSSL_BASE)/include
LIBOPENSSL_GLOBAL_INCLUDES-y     += -I$(LIBOPENSSL_SRC)/include
CINCLUDES-$(CONFIG_LIBOPENSSL)   += $(LIBOPENSSL_GLOBAL_INCLUDES-y)
CXXINCLUDES-$(CONFIG_LIBOPENSSL) += $(LIBOPENSSL_GLOBAL_INCLUDES-y)

LIBSSL_COMMON_INCLUDES-y         += -I$(LIBOPENSSL_SRC)
LIBSSL_COMMON_INCLUDES-y         += -I$(LIBOPENSSL_SRC)/include
LIBSSL_CINCLUDES                 += $(LIBSSL_COMMON_INCLUDES-y)
LIBSSL_CXXINCLUDES               += $(LIBSSL_COMMON_INCLUDES-y)

LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBSSL_BASE)/crypto
LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBOPENSSL_SRC)
LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBOPENSSL_SRC)/crypto/include
LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBOPENSSL_SRC)/include
LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBOPENSSL_SRC)/crypto
LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBOPENSSL_SRC)/crypto/ec/curve448/arch_32
LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBOPENSSL_SRC)/crypto/ec/curve448
LIBCRYPTO_COMMON_INCLUDES-y      += -I$(LIBOPENSSL_SRC)/crypto/modes
LIBCRYPTO_CINCLUDES              += $(LIBCRYPTO_COMMON_INCLUDES-y)
LIBCRYPTO_CXXINCLUDES            += $(LIBCRYPTO_COMMON_INCLUDES-y)

################################################################################
# Global flags
################################################################################
LIBOPENSSL_SUPPRESS_FLAGS += -Wno-unused-parameter
LIBOPENSSL_SUPPRESS_FLAGS += -Wno-unused-variable
LIBOPENSSL_SUPPRESS_FLAGS += -Wno-unused-value
LIBOPENSSL_SUPPRESS_FLAGS += -Wno-unused-function
LIBOPENSSL_SUPPRESS_FLAGS += -Wno-missing-field-initializers
LIBOPENSSL_SUPPRESS_FLAGS += -Wno-implicit-fallthrough

LIBSSL_CFLAGS-y      += $(LIBOPENSSL_SUPPRESS_FLAGS) -Wno-pointer-to-int-cast -Wno-int-to-pointer-cast
LIBSSL_CXXFLAGS-y    += $(LIBOPENSSL_SUPPRESS_FLAGS)

LIBCRYPTO_CFLAGS-y   += $(LIBOPENSSL_SUPPRESS_FLAGS) -Wno-pointer-to-int-cast -Wno-int-to-pointer-cast
LIBCRYPTO_CFLAGS-$(call have_gcc) += -Wno-maybe-uninitialized
LIBCRYPTO_CXXFLAGS-y += $(LIBOPENSSL_SUPPRESS_FLAGS)

LIBOPENSSL_DEFINES   += -DOPENSSL_USE_NODELETE
LIBOPENSSL_DEFINES   += -DL_ENDIAN
LIBOPENSSL_DEFINES   += -DOPENSSL_PIC
LIBOPENSSL_DEFINES   += -DOPENSSL_CPUID_OBJ
LIBOPENSSL_DEFINES   += -DOPENSSL_IA32_SSE2
LIBOPENSSL_DEFINES   += -DOPENSSL_BN_ASM_MONT
LIBOPENSSL_DEFINES   += -DOPENSSL_BN_ASM_MONT5
LIBOPENSSL_DEFINES   += -DOPENSSL_BN_ASM_GF2m
LIBOPENSSL_DEFINES   += -DSHA1_ASM
LIBOPENSSL_DEFINES   += -DSHA256_ASM
LIBOPENSSL_DEFINES   += -DSHA512_ASM
LIBOPENSSL_DEFINES   += -DKECCAK1600_ASM
LIBOPENSSL_DEFINES   += -DRC4_ASM
LIBOPENSSL_DEFINES   += -DMD5_ASM
LIBOPENSSL_DEFINES   += -DAESNI_ASM
LIBOPENSSL_DEFINES   += -DVPAES_ASM
LIBOPENSSL_DEFINES   += -DGHASH_ASM
LIBOPENSSL_DEFINES   += -DECP_NISTZ256_ASM
LIBOPENSSL_DEFINES   += -DX25519_ASM
LIBOPENSSL_DEFINES   += -DPOLY1305_ASM
LIBOPENSSL_DEFINES   += -DOPENSSLDIR=\"/usr/local/ssl\"
LIBOPENSSL_DEFINES   += -DENGINESDIR=\"/usr/local/lib/engines-1.1\"
LIBOPENSSL_DEFINES   += -DNDEBUG

LIBSSL_CFLAGS-y      += $(LIBOPENSSL_DEFINES)
LIBSSL_CXXFLAGS-y    += $(LIBOPENSSL_DEFINES)

LIBCRYPTO_CFLAGS-y   += $(LIBOPENSSL_DEFINES)
LIBCRYPTO_CXXFLAGS-y += $(LIBOPENSSL_DEFINES)
LIBCRYPTO_ASFLAGS-y += -Wno-unused-command-line-argument

